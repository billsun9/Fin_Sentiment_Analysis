{
  "manifest_version": 2,

  "name": "Wisdom",
  "description": "We love react",
  "version": "0.0.1",
  "background": {
		"scripts":["background.js"]
	},
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Open the popup",
    "default_icon": "logo192.png"
  },
  "permissions": [
    "tabs",
    "storage",
    "<all_urls>"
  ]
}